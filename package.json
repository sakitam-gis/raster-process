{
  "name": "@sakitam-gis/raster-process",
  "description": "A raster data process utilities",
  "version": "1.0.0",
  "private": false,
  "type": "commonjs",
  "license": "BSD 3-Clause",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "commonjs": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
  },
  "author": "sakitam-fdd <smilefdd@gmail.com>",
  "repository": {
    "url": "https://github.com/sakitam-gis/raster-process.git"
  },
  "scripts": {
    "dev": "npm run build -- --watch",
    "build": "tsup-node src/index.ts --sourcemap --dts --format esm,cjs",
    "prepublishOnly": "npm run build",
    "test": "npm run build && npm run test-only",
    "test:process": "tsx watch test_process/index.js | pino-pretty --colorize",
    "test-only": "vitest run --no-isolate",
    "coverage": "vitest run --coverage",
    "semantic-release": "npx -p node@v18-lts -c \"npx semantic-release --branches master\""
  },
  "dependencies": {
    "@sakitam-gis/affine": "^1.1.0",
    "@sakitam-gis/mercantile": "^1.0.0",
    "@mapbox/mbtiles": "^0.12.1",
    "ali-oss": "^6.20.0",
    "fs-extra": "^11.2.0",
    "gdal-async": "^3.9.0",
    "ndarray": "^1.0.19",
    "cwise": "^1.0.10",
    "lodash": "^4.17.21",
    "ndarray-gdal": "^1.5.0",
    "ndarray-concat-rows": "^1.0.1",
    "ndarray-concat-cols": "^1.0.0",
    "tapable": "^2.2.1",
    "pino": "^9.3.2"
  },
  "devDependencies": {
    "@aurorafe/eslint-config-base": "^1.1.0",
    "@aurorafe/eslint-config-ts": "^1.1.0",
    "@aurorafe/eslint-config-prettier": "^1.1.0",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.1.3",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@types/node": "^18.19.43",
    "@types/lodash": "^4.17.7",
    "@types/ndarray": "^1.0.14",
    "@stdlib/types": "^0.3.2",
    "@vitest/coverage-v8": "^2.0.5",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "tsx": "^4.16.5",
    "tsup": "^8.2.4",
    "typescript": "^5.3.5",
    "vitest": "^2.0.5"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  }
}
